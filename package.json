{
  "name": "2018.dotyork.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "query-string": "^6.1.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.0",
    "react-scroll-into-view-if-needed": "^2.1.6"
  },
  "scripts": {
    "dev": "npm-run-all -p watch-css start",
    "start": "react-scripts start",
    "build": "node-sass-chokidar src/ -o src/ && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "prettier": "^1.14.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
